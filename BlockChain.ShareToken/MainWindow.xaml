<Window x:Class="BlockChain.ShareToken.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlockChain.ShareToken"
        xmlns:local_share="clr-namespace:BlockChain.Share;assembly=BlockChain.Share"
        mc:Ignorable="d" Closing="Window_Closing"
        Loaded="Window_Loaded" WindowStartupLocation="CenterScreen"
        Title="Share Token and Decentralized Exchange"  MinHeight="800" MinWidth="1280" Height="800" Width="1280">

    <Window.Resources>

        <local_share:LanguageHelper x:Key="LanguageHelper"></local_share:LanguageHelper>
        <local_share:RRColorConvert x:Key="RRColorConvert"></local_share:RRColorConvert>

        <Style TargetType="Button">
            <Setter Property="Margin" Value="2" />
            <Setter Property="Height" Value="35" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="BorderThickness" Value="0" />
        </Style>

        <Style TargetType="Label">
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="Margin" Value="2,0" />
        </Style>

        <Style TargetType="CheckBox">
            <Setter Property="Margin" Value="2" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
        </Style>


        <Style x:Key="TextStyle" TargetType="TextBlock">
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="Margin" Value="5" />
        </Style>

        <Style x:Key="NumberStyle" TargetType="TextBlock">
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="Margin" Value="10,5" />
            <Setter Property="TextAlignment" Value="Right"/>
        </Style>

        <Style x:Key="NumberStyle2" TargetType="TextBlock">
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="TextAlignment" Value="Center"/>
        </Style>

        <!--<Style TargetType="CheckBox">
            <Setter Property="Margin" Value="5,0" />
        </Style>-->

        <Style x:Key="CheckBoxStyle" TargetType="CheckBox">
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="IsHitTestVisible" Value="False"/>
        </Style>

        <Style x:Key="TextStyle2" TargetType="TextBlock">
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="Margin" Value="2" />
        </Style>

        <Style TargetType="ComboBox">
            <Setter Property="Margin" Value="2" />
            <Setter Property="Height" Value="35" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
        </Style>


        <Style TargetType="TextBox">
            <Setter Property="Margin" Value="2" />
            <Setter Property="Height" Value="35" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
        </Style>

    </Window.Resources>

    <Grid>
        <Grid >
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition Height="1"></RowDefinition>
                <RowDefinition Height="40"></RowDefinition>
            </Grid.RowDefinitions>

            <TabControl Grid.Row="0" Name="TabControlMain" SelectionChanged="OnTabControlSelectionChanged">

                <TabItem  Width="160">

                    <TabItem.Header >
                        <Grid  HorizontalAlignment="Left"  Width="150">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="32"></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Image Grid.Column="0" Source="Image\exchange.jfif" Width="30" Height="30"></Image>

                            <Label Grid.Column="1" Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='去中心化交易所'}" ></Label>
                        </Grid>
                    </TabItem.Header>                    
                    <!--todo-->

                    <TabControl x:Name="TabControl0Dex" Margin="0,5" SelectionChanged="TabControl0Dex_SelectionChanged">
                        <TabControl.Resources>
                            <Style TargetType="{x:Type TabPanel}">
                                <Setter Property="HorizontalAlignment" Value="Center" />
                            </Style>
                        </TabControl.Resources>

                        <TabItem >
                            <TabItem.Header>
                                <Grid MinWidth="120" Height="25">
                                    <Label Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='交易对列表'}" Margin="0"></Label>
                                </Grid>
                            </TabItem.Header>

                            <Grid Margin="10">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="60"></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <!--<RowDefinition  Height="30"></RowDefinition>-->
                                </Grid.RowDefinitions>

                                <Grid Grid.Row="0" HorizontalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                        <ColumnDefinition Width="400"></ColumnDefinition>
                                        <ColumnDefinition Width="120"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Label Grid.Column="0" Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='请输入过滤条件:'}" ></Label>
                                    <TextBox Grid.Column="1"  Name="TextBoxFilter00"  Margin="5,0"></TextBox>
                                    <Button Grid.Column="2" Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='查询'}" Click="Click_SearchDivPairList"  ></Button>
                                </Grid>

                                <DataGrid Grid.Row="1" x:Name="DataGridPairList" AutoGenerateColumns="False" CanUserAddRows="False" CanUserDeleteRows="False" SelectionMode="Single"  MinWidth="1200" MaxWidth="1800">
                                    <DataGrid.Columns>

                                        <DataGridTemplateColumn Header="Share Token"  CanUserSort="True"  SortMemberPath="_shareToken" MinWidth="160">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button Click="TagOnGotoAddress" Tag="{Binding _shareToken}" HorizontalAlignment="Left" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}" MinWidth="150" >
                                                        <Grid>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                                                <ColumnDefinition></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <Image  Grid.Column="0" Source="{Binding ShareTokenIconLocalFileName}" Height="30" Width="30"></Image>
                                                            <Label Grid.Column="1" Content="{Binding ShareTokenSymbol}"></Label>
                                                            <Label Grid.Column="2" Content="{Binding ShareTokenName}"></Label>
                                                        </Grid>
                                                    </Button>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>

                                        <DataGridTemplateColumn  Header="Share Token Liquidity Amount"  CanUserSort="True"  SortMemberPath="ShareTokenCurrentLiqAmount" MinWidth="100">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Label Content="{Binding ShareTokenCurrentLiqAmount}"  HorizontalAlignment="Right"></Label>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>

                                        <DataGridTemplateColumn Header="Div Token" CanUserSort="True"  SortMemberPath="DivTokenAddress" MinWidth="100">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button Click="TagOnGotoAddress" Tag="{Binding DivTokenAddress}" HorizontalAlignment="Left" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}" Margin="3,0">
                                                        <Grid >
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="32"></ColumnDefinition>
                                                                <ColumnDefinition ></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <Image  Grid.Column="0" Source="{Binding DivTokenImagePath}" Height="30" Width="30"></Image>
                                                            <Label Grid.Column="1" Content="{Binding DivTokenSymbol}"></Label>
                                                        </Grid>
                                                    </Button>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>

                                        <DataGridTemplateColumn  Header="DivToken Liquidity Amount"  CanUserSort="True"  SortMemberPath="DivTokenCurrentLiqAmount" MinWidth="100">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Label Content="{Binding DivTokenCurrentLiqAmount}" HorizontalAlignment="Right"></Label>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>

                                        <DataGridTemplateColumn  Header="Price0 (1 ShareToken = ? DivToken)"  CanUserSort="True"  SortMemberPath="Price0" MinWidth="50">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Label Content="{Binding Price0}" ContentStringFormat="{}{0:N18}" HorizontalAlignment="Right"></Label>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>

                                        <DataGridTemplateColumn  Header="Price1 (1 DivToken = ? ShareToken)"  CanUserSort="True"  SortMemberPath="Price1" MinWidth="50">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Label Content="{Binding Price1}" ContentStringFormat="{}{0:N18}" HorizontalAlignment="Right"></Label>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>

                                        <!--<DataGridTemplateColumn>
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='刷新价格'}" Tag="{Binding _pair}" Click="Click_RefreshPrice" Margin="5,0" ></Button>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>-->

                                        <DataGridTemplateColumn MinWidth="160" MaxWidth="160">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='详情（交易/流动性/分红）'}" Tag="{Binding _newPair}" Click="Click_PairAddress" Margin="5,3" Width="150" ></Button>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>

                                    </DataGrid.Columns>
                                </DataGrid>

                            </Grid>

                        </TabItem>

                        <TabItem>
                            <TabItem.Header>
                                <Grid MinWidth="120" Height="25">
                                    <Label Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='交易、流动性和分红'}" Margin="0"></Label>
                                </Grid>
                            </TabItem.Header>

                            <ScrollViewer Grid.Row="1">
                                <StackPanel Width="1200"  VerticalAlignment="Top">

                                    <Grid Height="250" Margin="10" Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="200" ></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition Width="200" ></ColumnDefinition>
                                            <ColumnDefinition ></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>

                                        <Border  Grid.Row="1" Grid.Column="0"  Background="WhiteSmoke"></Border>
                                        <Border  Grid.Row="3" Grid.Column="0" Background="WhiteSmoke"></Border>
                                        <Border  Grid.Row="1" Grid.Column="2"  Background="WhiteSmoke"></Border>
                                        <Border  Grid.Row="3" Grid.Column="2" Background="WhiteSmoke"></Border>
                                       
                                        <Border  Grid.Row="0" Grid.Column="1"  Background="WhiteSmoke"></Border>
                                        <Border  Grid.Row="2" Grid.Column="1" Background="WhiteSmoke"></Border>
                                        <Border  Grid.Row="4" Grid.Column="1"  Background="WhiteSmoke"></Border>
                                        <Border  Grid.Row="0" Grid.Column="3"  Background="WhiteSmoke"></Border>
                                        <Border  Grid.Row="2" Grid.Column="3" Background="WhiteSmoke"></Border>
                                        <Border  Grid.Row="4" Grid.Column="3"  Background="WhiteSmoke"></Border>


                                        <Label Grid.Row="0" Grid.Column="0" Content="Pair:" HorizontalAlignment="Right"></Label>
                                        <Button Grid.Row="0" Grid.Column="1" Name="ButtonPair01"  Click="Click_GoToAddress" HorizontalAlignment="Left" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}" Foreground="Blue"></Button>
                                        <Label Grid.Row="0" Grid.Column="2" Content="IsPaused:" HorizontalAlignment="Right"></Label>
                                        <Label Grid.Row="0" Grid.Column="3" Name="LabelIsPaused" HorizontalAlignment="Left"></Label>

                                        <Button Grid.Row="0" Grid.Column="3" Width="75" HorizontalAlignment="Right" Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='刷新'}" Click="Click_RefreshPair" ></Button>

                                        <Label Grid.Row="1" Grid.Column="0" Content="Share Token:" HorizontalAlignment="Right" ></Label>
                                        <StackPanel Grid.Row="1" Grid.Column="1" Name="StackPanelShareToken01" Orientation="Horizontal" HorizontalAlignment="Left" >
                                            <Button Name="ButtonShareToken01" MaxWidth="200" Tag="{Binding Address}"   ToolTip="{Binding Address}"  Click="Click_GoToAddress2" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"  HorizontalAlignment="Left">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="32"></ColumnDefinition>
                                                        <ColumnDefinition ></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>
                                                    <Image Grid.Column="0" Source="{Binding ImagePath}" Width="30" Height="30"></Image>
                                                    <Label Grid.Column="1" Content="{Binding Symbol}" VerticalAlignment="Center" Foreground="Blue"></Label>
                                                </Grid>
                                            </Button>
                                            <Label  Grid.Row="0" x:Name="LabelShareTokenLiq"  ContentStringFormat="N18"></Label>
                                        </StackPanel>

                                        <Label Grid.Row="1" Grid.Column="2" Content="Div Token:" HorizontalAlignment="Right"></Label>
                                        <StackPanel Grid.Row="1" Grid.Column="3" Name="StackPanelDivToken01" Orientation="Horizontal" HorizontalAlignment="Left">
                                            <Button  Name="ButtonDivToken01"  MaxWidth="200"   Tag="{Binding Address}"  ToolTip="{Binding Address}" Click="Click_GoToAddress2" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"  HorizontalAlignment="Left">
                                                <Grid >
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="32"></ColumnDefinition>
                                                        <ColumnDefinition ></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>
                                                    <Image Grid.Column="0" Source="{Binding ImagePath}" Height="30" Width="30" ></Image>
                                                    <Label Grid.Column="1"  Content="{Binding Symbol}" VerticalAlignment="Center" Foreground="Blue"></Label>
                                                </Grid>
                                            </Button>
                                            <Label  Grid.Row="0" x:Name="LabelDivTokenLiq"  ContentStringFormat="N18"></Label>
                                        </StackPanel>

                                        <Label  Grid.Row="2" Grid.Column="0" Content="Price0:" HorizontalAlignment="Right"></Label>
                                        <Label  Grid.Row="2" Grid.Column="1" Name="LabelPrice0" Content="***"></Label>

                                        <Label  Grid.Row="2" Grid.Column="2" Content="Price1:" HorizontalAlignment="Right"></Label>
                                        <Label  Grid.Row="2" Grid.Column="3" Name="LabelPrice1" Content="***"></Label>

                                        <Label Grid.Row="3" Grid.Column="0" Content="Select Account:" HorizontalAlignment="Right" Foreground="Green" ></Label>
                                        <ComboBox  x:Name="ComboBoxAddress01"   Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="3"  Width="500" HorizontalAlignment="Left" SelectionChanged="ComboBoxAddress01_SelectionChanged"  >
                                            <ComboBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid Width="{Binding  ElementName=ComboBoxAddress01, Path=Width}">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="160"></ColumnDefinition>
                                                            <ColumnDefinition ></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <Label Grid.Column="0" Content="{Binding AddressAlias}"></Label>
                                                        <Label Grid.Column="1" Content="{Binding Address}" VerticalAlignment="Center"></Label>
                                                    </Grid>
                                                </DataTemplate>
                                            </ComboBox.ItemTemplate>
                                        </ComboBox>

                                        <Label Grid.Row="4" Grid.Column="0" Content="Account Balance:" HorizontalAlignment="Right" Foreground="Green" ></Label>

                                        <Grid Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="100" ></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition Width="100" ></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition Width="100" ></ColumnDefinition>
                                                <ColumnDefinition ></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Label Grid.Column="0" Content="ETH:" HorizontalAlignment="Right" Foreground="Green" ></Label>
                                            <Label Grid.Column="1" x:Name="LabelAccountEth" Content=""></Label>
                                            <Label Grid.Column="2" x:Name="LabelAccountShareToken"  Content="Share Token:" HorizontalAlignment="Right" Foreground="Green" ></Label>
                                            <Label Grid.Column="3" x:Name="LabelAccountShare" Content=""></Label>
                                            <Label Grid.Column="4" Name="LabelAccountDivToken" Content="Div Token:" HorizontalAlignment="Right" Foreground="Green" ></Label>
                                            <Label Grid.Column="5" x:Name="LabelAccountDiv" Content=""></Label>

                                        </Grid>

                                    </Grid>

                                    <Expander   Name="ExpanderSwap1" Expanded="Expanded_Swap1" Margin="10" MinHeight="40" >
                                        <Expander.Header>
                                            <TextBlock Text="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='交易'}" Background="WhiteSmoke" Width="1200"></TextBlock>
                                        </Expander.Header>

                                            <Grid Height="250" Width="800" >
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="80"></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>

                                            <Label Grid.Row="0" Grid.Column="0" Content="From:"></Label>
                                            <Grid  Grid.Row="0" Grid.Column="1">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>

                                                <ComboBox  Name="ComboBoxFromToken"    Grid.Row="1" Grid.Column="0"  SelectionChanged="ComboBoxFromTokenOnSelectionChanged">
                                                    <ComboBox.ItemTemplate>
                                                        <DataTemplate>
                                                            <Grid Width="{Binding  ElementName=ComboBoxFromToken, Path=Width}">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="32"></ColumnDefinition>
                                                                    <ColumnDefinition ></ColumnDefinition>
                                                                </Grid.ColumnDefinitions>
                                                                <Image  Grid.Column="0" Source="{Binding ImagePath}" Height="30" Width="30"></Image>
                                                                <Label Grid.Column="1" Content="{Binding Symbol}"></Label>
                                                            </Grid>
                                                        </DataTemplate>
                                                    </ComboBox.ItemTemplate>
                                                </ComboBox>

                                                <TextBox Grid.Row="1" Grid.Column="1" x:Name="TextBoxFromToken"  HorizontalContentAlignment="Right"  Foreground="Blue"  LostFocus="TextBoxFromTokenOnLostFocus"></TextBox>
                                            </Grid>

                                            <Label Grid.Row="1" Grid.Column="0" Content="To:"></Label>
                                            <Grid  Grid.Row="1" Grid.Column="1">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>

                                                <Button  Name="ButtonToToken"    Grid.Row="1" Grid.Column="0" Tag="{Binding Address}"  Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}" HorizontalAlignment="Left">
                                                    <Grid HorizontalAlignment="Left">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="32"></ColumnDefinition>
                                                            <ColumnDefinition ></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <Image  Grid.Column="0" Source="{Binding ImagePath}" Height="30" Width="30"></Image>
                                                        <Label Grid.Column="1" Content="{Binding Symbol}"></Label>
                                                    </Grid>
                                                </Button>

                                                <Label Grid.Row="1" Grid.Column="1" x:Name="LabelToToken" HorizontalAlignment="Right" Foreground="Blue"></Label>

                                            </Grid>

                                            <Grid  Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="80"></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition Width="80"></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <Label Grid.Column="0" Content="Price:"></Label>
                                                <Label  Grid.Column="1"  x:Name="LabelPrice"></Label>

                                              
                                            </Grid>

                                            <Grid  Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="120"></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition Width="120"></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <Label Grid.Column="0" Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='滑点容差（%）:'}"></Label>
                                                <TextBox Grid.Column="1"  x:Name="TextBoxSlippage" Text="0.5" Width="100"  HorizontalAlignment="Left" HorizontalContentAlignment="Right"></TextBox>

                                                <Label Grid.Column="2" Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='交易截至期限(分钟):'}"></Label>
                                                <TextBox  Grid.Column="3"  x:Name="TextBoxTxDeadline"  Text="20"  Width="100"  HorizontalAlignment="Left" HorizontalContentAlignment="Right"></TextBox>
                                            </Grid>

                                            <Button Grid.Row="4" Grid.Column="1"  Content="兑换（Swap）"  Click="ButtonSwapOnClick" Height="40" Width="200" HorizontalAlignment="Left"></Button>

                                        </Grid>

                                    </Expander>

                                    <Expander Name="ExpanderLiq2" Margin="10" MinHeight="40"   Expanded="Expanded_Liq2" >
                                        <Expander.Header>
                                            <TextBlock Text="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='流动性'}" Background="WhiteSmoke" Width="1200"></TextBlock>
                                        </Expander.Header>

                                        <TabControl Name="TabControlLiq" SelectionChanged="TabControlLiq_SelectionChanged" Width="800" Margin="0,5">

                                            <TabItem Header="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='增加流动性'}" Width="120">

                                                <Grid Height="250" Margin="5" >
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="100"></ColumnDefinition>
                                                        <ColumnDefinition></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition></RowDefinition>
                                                        <RowDefinition></RowDefinition>
                                                        <RowDefinition></RowDefinition>
                                                        <RowDefinition Height="50"></RowDefinition>
                                                    </Grid.RowDefinitions>

                                                    <Label Grid.Row="0" Grid.Column="0" Content="ShareToken:"></Label>
                                                    <Grid  Grid.Row="0" Grid.Column="1">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition  Width="200"></ColumnDefinition>
                                                            <ColumnDefinition  Width="200"></ColumnDefinition>
                                                            <ColumnDefinition></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>

                                                        <Button  Name="ButtonTokenA"    Grid.Row="1" Grid.Column="0" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"  HorizontalAlignment="Left">
                                                            <Grid HorizontalAlignment="Left">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="32"></ColumnDefinition>
                                                                    <ColumnDefinition ></ColumnDefinition>
                                                                </Grid.ColumnDefinitions>
                                                                <Image  Grid.Column="0" Source="{Binding ImagePath}" Height="30" Width="30"></Image>
                                                                <Label Grid.Column="1" Content="{Binding Symbol}"></Label>
                                                            </Grid>

                                                        </Button>

                                                        <TextBox Grid.Row="1" Grid.Column="1" x:Name="TextBoxTokenA"  HorizontalContentAlignment="Right"  Foreground="Blue"  LostFocus="TextBoxTokenAOnLostFocus"></TextBox>
                                                        <Label Grid.Row="1" Grid.Column="2" x:Name="LabelTokenAMax"  HorizontalContentAlignment="Right" Foreground="Gray"></Label>
                                                    </Grid>

                                                    <Label Grid.Row="1" Grid.Column="0" Content="DivToken:"></Label>
                                                    <Grid  Grid.Row="1" Grid.Column="1">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="100"></ColumnDefinition>
                                                            <ColumnDefinition Width="100"></ColumnDefinition>
                                                            <ColumnDefinition Width="200"></ColumnDefinition>
                                                            <ColumnDefinition Width="100"></ColumnDefinition>
                                                            <ColumnDefinition></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>

                                                        <Button  Name="ButtonTokenB" Grid.Column="0" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}" HorizontalAlignment="Left">
                                                            <Grid HorizontalAlignment="Left">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="32"></ColumnDefinition>
                                                                    <ColumnDefinition ></ColumnDefinition>
                                                                </Grid.ColumnDefinitions>
                                                                <Image  Grid.Column="0" Source="{Binding ImagePath}" Height="30" Width="30"></Image>
                                                                <Label Grid.Column="1" Content="{Binding Symbol}"></Label>
                                                            </Grid>
                                                        </Button>

                                                        <Label Grid.Column="1" Content="Max:" HorizontalContentAlignment="Right" Foreground="Gray"></Label>
                                                        <TextBox Grid.Column="2" x:Name="TextBoxTokenB"  HorizontalContentAlignment="Right"  Foreground="Blue" TextChanged="TextBoxTokenB_TextChanged"></TextBox>
                                                        <Label Grid.Column="3" Content="Min:" HorizontalContentAlignment="Right" Foreground="Gray"></Label>
                                                        <Label  Grid.Column="4" x:Name="LabelTokenBMax"  HorizontalContentAlignment="Left" Foreground="Blue"></Label>
                                                    </Grid>

                                                    <Grid  Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="120"></ColumnDefinition>
                                                            <ColumnDefinition></ColumnDefinition>
                                                            <ColumnDefinition Width="120"></ColumnDefinition>
                                                            <ColumnDefinition></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <Label Grid.Column="0" Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='滑点容差（%）:'}"></Label>
                                                        <TextBox Grid.Column="1"  x:Name="TextBoxSlippageLiq" Text="1" Width="100"  HorizontalAlignment="Left" HorizontalContentAlignment="Right" LostFocus="TextBoxSlippageLiq_LostFocus"></TextBox>

                                                        <Label Grid.Column="2" Content="{Binding Converter={ StaticResource LanguageHelper}, ConverterParameter='交易截至期限(分钟):'}"></Label>
                                                        <TextBox  Grid.Column="3"  x:Name="TextBoxTxDeadlineLiq"  Text="20"  Width="100"  HorizontalAlignment="Left" HorizontalContentAlignment="Right"></TextBox>
                                                    </Grid>

